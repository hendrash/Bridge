{
	"configurations": [
		{
			"type": "pwa-node",
			"name": "truffle deploy",
			"request": "launch",
			"runtimeExecutable": "truffle",
			"runtimeArgs": [
				"deploy"
			],
			"envFile": "${workspaceFolder}/.env"
		},
		{
			"type": "pwa-node",
			"name": "truffle test",
			"request": "launch",
			"runtimeExecutable": "truffle",
			"runtimeArgs": [
				"test",
			],
			"envFile": "${workspaceFolder}/.env"
		},
		{
			"type": "pwa-node",
			"name": "truffle --network testnet migrate",
			"request": "launch",
			"runtimeExecutable": "truffle",
			"runtimeArgs": [
				"test",
				"--network",
				"testnet",
			],
			"envFile": "${workspaceFolder}/.env"
		},
		{
			"type": "pwa-node",
			"name": "ts-node mockServer/mockServer",
			"request": "launch",
			"runtimeExecutable": "ts-node",
			"runtimeArgs": [
				"mockServer/mockServer.ts",
			],
			"envFile": "${workspaceFolder}/.env"
		}
	]
}